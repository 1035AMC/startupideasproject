<html>
<head>
    <link rel="stylesheet" href="../../../libs/bootstrap/dist/css/bootstrap.min.css">
    <link rel="stylesheet" type="text/css" href="../../../stylesheets/style.css">
    <link rel="stylesheet" type="text/css" href="../../../libs/font-awesome/css/font-awesome.css">

    <!-- JS -->
    <script src="../../../libs/angular/angular.min.js"></script>
    <script src="../../../libs/jquery/dist/jquery.min.js"></script>
    <script src="../../../libs/angular-route/angular-route.min.js"></script>
    <script src="../../javascripts/voting.js"></script>


</head>

    <%- include('header.ejs') %>


<main role="main">
  <div class="container">
    <ul class="list-group">
    <% if (user && startupideas){startupideas.forEach(function(idea){ %>
      <li class="list-group-item">
        <div class="container">

        <i class="fa fa-thumbs-o-up"  id="<%= idea._id %>"></i>
        <span style="font-size: 15px"><%= idea.vote %></span>
        <i class="fa fa-thumbs-o-down" id="<%=idea._id %>"></i>

        </div>
        <h3>  
        <a href="/startupidea/<%= idea._id %>">
          <%= idea.name %>
        </a>
      </h3>
      <div><%= idea.info %></div>
    </li>
    <% });}%>
    </ul>
  </main>
</div>

</html>