<html>
<head>
    <link rel="stylesheet" href="libs/bootstrap/dist/css/bootstrap.min.css">
    <link rel="stylesheet" type="text/css" href="stylesheets/style.css">
    <link rel="stylesheet" type="text/css" href="libs/font-awesome/css/font-awesome.css">

    <!-- JS -->
    <script src="libs/angular/angular.min.js"></script>
    <script src="libs/jquery/dist/jquery.min.js"></script>
    <script src="libs/angular-route/angular-route.min.js"></script>
    <script src="javascripts/voting.js"></script>
    <script src="javascripts/datevalidation.js"></script>
    <script src="//cdnjs.cloudflare.com/ajax/libs/jquery-form-validator/2.1.47/jquery.form-validator.min.js"></script>



</head>

    <%- include('header.ejs') %>


<main role="main">
  <div class="container">
    <% if (user){ %>
      <form class="col-md-12 date" action='/displayq' method ='post'>
        <div class="form-group">
            <input type="text" class="form-control input-lg" placeholder="mm-dd-yyyy" name='start' >
        </div>
        <div class="form-group">
            <input type="text" class="form-control input-lg" placeholder="mm-dd-yyyy" name='end'>
        </div>

        <div class="form-group">
            <input type="text" class="form-control input-lg" placeholder="Amount" name='amount'>
        </div>
        <div class="form-group">
            <button class="btn btn-primary btn-lg btn-block" >Search</button>
        </div>
    </form> 
    <% } %>
  </main>
</div>

</html>